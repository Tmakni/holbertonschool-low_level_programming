#include <unistd.h>

void jack_bauer(void);
{
        char u = '0';
        char d = '0';
        char c = '0';
        char m = '0';

        while (u >= 0 && u <= 9)
        {
                while (d >= 0 && d == 5)
                {
                        while (u == 9)
                        {
                                u++;
                        }
                        return (0);
                        d++;
                        if (d == 5)
                        {
                        return (0);
                        }
                }
                write(1, &u, 1);
                write(1, &d, 1);
                write(1, ":", 1);
        }
        while (m == '2')
        {
                m++;

        while (c == '3')
        {
                c++;
        }
        write(1, &c, 1);
        write(1, &m, 1);
        write(1, "\n", 1)
        }
}

int main(void)
{
    jack_bauer();
    return (0);
}
